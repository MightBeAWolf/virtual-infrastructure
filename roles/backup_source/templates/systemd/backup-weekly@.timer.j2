# This file was generated with Ansible
[Unit]
Description=Trigger a weekly backup using the kit %i, if it hasn't last completed within the last 2 days

[Timer]
Unit=backup@%i.service
OnCalendar=weekly
OnUnitInactiveSec=2 days
AccuracySec=6h
RandomizedDelaySec=3 days

[Install]
WantedBy=timers.target
